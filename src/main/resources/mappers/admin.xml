<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.restaurant.repository.AdminRepository">

    <select id="findByAdminLogin" parameterType="String" resultType="com.korit.restaurant.entity.admin.AdminMst">
        select adminId, adminPw
        from user
        where adminId = #{adminId} and adminPw = #{adminPw}
    </select>

</mapper>